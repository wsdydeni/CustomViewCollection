<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowBackground">#9000</item>
    </style>

    <declare-styleable name="LoadingCircleView">
        <attr name="backColor" format="color"/>
        <attr name="loadingColor" format="color"/>
        <attr name="successColor" format="color"/>
        <attr name="failureColor" format="color"/>
        <attr name="paintWidth" format="float"/>
    </declare-styleable>

</resources>